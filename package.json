{
  "name": "@titelmedia/node-fetch",
  "version": "3.1.1",
  "description": "A light-weight module that brings the Fetch API to node.js; Forked from https://npm.im/node-fetch",
  "main": "lib/index",
  "module": "lib/index.mjs",
  "files": [
    "lib/index.js",
    "lib/index.mjs",
    "lib/index.es.js"
  ],
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "build": "cross-env BABEL_ENV=rollup rollup -c",
    "prepare": "npm run build",
    "test": "cross-env BABEL_ENV=test mocha --require babel-register test/test.js",
    "report": "cross-env BABEL_ENV=coverage nyc --reporter lcov --reporter text mocha -R spec test/test.js",
    "coverage": "cross-env BABEL_ENV=coverage nyc --reporter json --reporter text mocha -R spec test/test.js && codecov -f coverage/coverage-final.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/titel-media/node-fetch.git"
  },
  "keywords": [
    "fetch",
    "http",
    "promise"
  ],
  "author": "David Frank",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/titel-media/node-fetch/issues"
  },
  "homepage": "https://github.com/titel-media/node-fetch",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.16.3",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-iterator": "^3.0.2",
    "chai-string": "^1.5.0",
    "codecov": "^3.0.0",
    "cross-env": "^5.1.3",
    "form-data": "^2.3.1",
    "is-builtin-module": "^1.0.0",
    "mocha": "^5.0.0",
    "nyc": "11.9.0",
    "parted": "^0.1.1",
    "promise": "^8.0.1",
    "resumer": "0.0.0",
    "rollup": "^0.63.4",
    "rollup-plugin-babel": "^3.0.3",
    "string-to-arraybuffer": "^1.0.0",
    "url-search-params": "^1.0.2",
    "whatwg-url": "^5.0.0"
  },
  "dependencies": {
    "web-streams-polyfill": "^2.1.1",
    "busboy": "^0.3.1",
    "formdata-node": "^2.4.0"
  },
  "peerDependencies": {
    "form-data": ">=3.0.0"
  }
}
